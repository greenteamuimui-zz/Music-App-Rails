<h1>Albums of <%= @band %></h1>
<%= @band.name %> <%= button_to "Edit", edit_band_url, method: :get %>
<%= button_to "Delete Band", band_url, method: :delete %>

<%= button_to "Go back to the Main Page", bands_url, method: :get  %>
<% @band.albums.each do |album| %>
  <ul>
    <li> <%= link_to album.name, album_url(album) %>  </li>
  </ul>
<% end %>
